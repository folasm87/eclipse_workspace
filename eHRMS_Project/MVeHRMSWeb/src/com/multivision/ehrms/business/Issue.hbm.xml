<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.multivision.ehrms.business.Issue" table="Issues">
		<id name="id" type="long">
			<generator class="native" />
		</id>
		<property name="issueId" column="issue_id" type="string" />
		<property name="issue" column="issue" type="string" />

		<many-to-one name="project" class="com.multivision.ehrms.business.Project" fetch="select">
            <column name="ID" not-null="true" />
        </many-to-one>
       

	</class>
</hibernate-mapping>