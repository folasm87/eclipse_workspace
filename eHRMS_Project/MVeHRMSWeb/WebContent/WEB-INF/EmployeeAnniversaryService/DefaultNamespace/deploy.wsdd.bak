<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from EmployeeAnniversaryService WSDL service -->

  <service name="EmployeeAnniversary" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://com.multivision.webservice"/>
      <parameter name="wsdlServiceElement" value="EmployeeAnniversaryService"/>
      <parameter name="schemaQualified" value="http://com.multivision.webservice"/>
      <parameter name="wsdlServicePort" value="EmployeeAnniversary"/>
      <parameter name="className" value="com.multivision.webservice.EmployeeAnniversarySoapBindingImpl"/>
      <parameter name="wsdlPortType" value="EmployeeAnniversary"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getCurrentMonthAnniversaryEmployeesName" qname="operNS:getCurrentMonthAnniversaryEmployeesName" xmlns:operNS="http://com.multivision.webservice" returnQName="retNS:getCurrentMonthAnniversaryEmployeesNameReturn" xmlns:retNS="http://com.multivision.webservice" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getCurrentMonthAnniversaryEmployeesName"/>

  </service>
</deployment>
