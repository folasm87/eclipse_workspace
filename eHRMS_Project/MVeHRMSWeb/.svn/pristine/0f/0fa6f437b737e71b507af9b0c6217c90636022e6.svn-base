package com.multivision.ehrms.util;

/**
 * The Class Attachment.
 */
public class Attachment {

	/** The attachmentpath. */
	private String attachmentpath;
	
	private String attachaddressproof;

	/** The attid. */
	private int attid;

	/**
	 * Gets the attachmentpath.
	 * 
	 * @return the attachmentpath
	 */
	public String getAttachmentpath() {
		return attachmentpath;
	}

	/**
	 * Sets the attachmentpath.
	 * 
	 * @param attachmentpath
	 *            the new attachmentpath
	 */
	public void setAttachmentpath(String attachmentpath) {
		this.attachmentpath = attachmentpath;
	}

	/**
	 * Gets the attid.
	 * 
	 * @return the attid
	 */
	public int getAttid() {
		return attid;
	}

	/**
	 * Sets the attid.
	 * 
	 * @param attid
	 *            the new attid
	 */
	public void setAttid(int attid) {
		this.attid = attid;
	}

	/**
	 * @param attachaddressproof the attachaddressproof to set
	 */
	public void setAttachaddressproof(String attachaddressproof) {
		this.attachaddressproof = attachaddressproof;
	}

	/**
	 * @return the attachaddressproof
	 */
	public String getAttachaddressproof() {
		return attachaddressproof;
	}

}
