package net.mv.dao;
import java.util.Set;

import javax.persistence.*;

@Entity
public class Task {
	
	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	@Column(name="ID")
	private long ID;
	
	@Column(name="TASK_ID")
	private String task_id;
	
	
	@Column
	private String name;
	private double cost;
	
	@OneToMany(cascade=CascadeType.ALL)
	@JoinColumn(name="ID")
	private Set<Project> projects;

}
