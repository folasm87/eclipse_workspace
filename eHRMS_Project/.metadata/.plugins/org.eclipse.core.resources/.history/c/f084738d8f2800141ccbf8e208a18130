package net.mv.dao;
import javax.persistence.*;

@Entity
public class Project {
	
	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	@Column(name="ID")
	private long ID;
	
	@Column(name="PROJ_ID")
	private String proj_id;
	
	
	@Column
	private String name;
	private String commissioner;
	private String desc;
	private String category;
	private String objective;

}
