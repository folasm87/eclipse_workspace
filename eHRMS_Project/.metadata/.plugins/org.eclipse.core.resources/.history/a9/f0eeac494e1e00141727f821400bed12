package com.multivision.ehrms.business;

import javax.persistence.*;

public class Task {

	@Id
	@GeneratedValue
	@Column(name="task_id")
	private long taskId;
	
	
	@Column
	private String name;
	
	@Column
	private String duration;
	
	@ManyToOne
	@JoinColumn(name="proj_id")
	private Project project;

	public Task(String name, String duration, Project project) {
		this.name = name;
		this.duration = duration;
		this.project = project;
	}
	
	
	
}
