package com.multivision.ehrms.business;

import javax.persistence.*;

public class Task {

	@Id
	@GeneratedValue
	@Column(name="task_id")
	private long taskId;
	
	
	@Column
	private String name;
	
	@Column
	private Time length;
	
	@ManyToOne
	@JoinColumn(name="proj_id")
	private Project project;
	
}
