package net.multivision.dao;


import java.io.*;
import java.util.*;
import java.sql.*;


public class IoDB {

	public static void main(String[] args) {
		File file = new File("src/net/multivision/dao/studentin.txt");
		
		FileReader fr;
		BufferedReader br;
		
		try{
			fr = new FileReader(file);
			br = new BufferedReader(fr);
			
			int line_count = 0;
			String line = null;
			while(true){
				String sql_table = "create table \"";
				line = br.readLine();
				
				line_count += 1;
				
				//String[] table_info;
				
				if (line == null){
					break;
				}else if (line_count == 1){
					sql_table = sql_table + line + "\"(";
				}else if (line_count == 2){
					
					ArrayList col_name;
					ArrayList col_type;
					ArrayList col_size;
					
					
					String[] table_info = line.split(":");
					int line_len = table_info.length;
					int j, k;
					
					for (int i = 0; i < line_len; i+=3){
						j = i+1;
						k = i+2;
						col_name.add(table_info[i]);
						
						if (j < line_len){
							
						}
					}
					//sql_table += ; 
				}
				
			}
		}
	}

}
