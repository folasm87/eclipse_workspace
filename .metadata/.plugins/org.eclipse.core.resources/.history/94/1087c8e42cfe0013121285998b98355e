package net.multivision.dao;


import java.io.*;
import java.util.*;


public class IoDB {

	public static void main(String[] args) {
		File file = new File("src/net/multivision/dao/studentin.txt");
		
		FileReader fr;
		BufferedReader br;
		
		try{
			fr = new FileReader(file);
			br = new BufferedReader(fr);
			
			int line_count = 0;
			String line = null;
			while(true){
				
				line = br.readLine();
				
				line_count += 1;
				
				if (line == null){
					break;
				}
				
			}
		}
	}

}
